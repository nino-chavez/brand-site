{
  "name": "nino-chavez-|-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ssr": "node server.js",
    "build": "vite build",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --config vite.config.ssr.ts --ssr src/entry-server.tsx --outDir dist/server",
    "build:ssr": "npm run build:client && npm run build:server",
    "build:analyze": "npm run build && node scripts/bundle-analyzer.js",
    "preview": "vite preview",
    "preview:ssr": "NODE_ENV=production node server.js",
    "validate": "node .claude/scripts/validate-changes.js",
    "validate:force": "npm run validate || true",
    "decision:template": "node .claude/scripts/log-decision.js template",
    "decision:log": "node .claude/scripts/log-decision.js log",
    "decision:list": "node .claude/scripts/log-decision.js list",
    "gallery:optimize": "node scripts/optimize-gallery-images.js",
    "gallery:exif": "node scripts/extract-exif.js",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:ui": "NODE_OPTIONS='--max-old-space-size=8192' vitest --ui",
    "test:run": "NODE_OPTIONS='--max-old-space-size=8192' vitest run",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --coverage",
    "test:traditional": "NODE_OPTIONS='--max-old-space-size=8192' vitest --config vitest-parallel.config.ts --mode traditional",
    "test:canvas": "NODE_OPTIONS='--max-old-space-size=8192' vitest --config vitest-parallel.config.ts --mode canvas",
    "test:parallel": "npm run test:traditional & npm run test:canvas",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:canvas": "playwright test tests/e2e/canvas-layout.spec.ts",
    "test:e2e:cross-browser": "playwright test tests/e2e/cross-browser.spec.ts",
    "test:motion": "playwright test --config=playwright.motion.config.ts",
    "test:motion:headed": "playwright test --config=playwright.motion.config.ts --headed",
    "test:motion:debug": "playwright test --config=playwright.motion.config.ts --debug",
    "test:motion:video": "playwright test --config=playwright.motion.config.ts --project=chromium-motion",
    "test:motion:report": "playwright show-report test-results/motion-report",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:migration": "npm run test:traditional && npm run test:canvas && npm run test:e2e:canvas",
    "test:runtime-errors": "tsx test/runtime-error-detection/index.ts",
    "test:runtime-errors:headless": "tsx test/runtime-error-detection/index.ts --headless=true",
    "test:runtime-errors:ci": "tsx test/runtime-error-detection/index.ts --ci --headless=true",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "stories:generate": "tsx scripts/generate-stories.ts",
    "stories:generate:force": "tsx scripts/generate-stories.ts --force",
    "stories:validate": "tsx scripts/validate-stories.ts",
    "stories:watch": "tsx scripts/watch-components.ts",
    "capture:components": "playwright test tests/screenshots/scripts/capture-components.spec.ts --config playwright.config.screenshots.ts",
    "capture:flows": "playwright test tests/screenshots/flows/ --config playwright.config.screenshots.ts",
    "capture:all": "npm run capture:components && npm run capture:flows",
    "capture:clean": "rm -rf tests/screenshots/output/*",
    "health": "tsx scripts/health-check.ts",
    "health:verbose": "tsx scripts/health-check.ts --verbose",
    "health:json": "tsx scripts/health-check.ts --json",
    "health:update": "tsx scripts/health-check.ts --verbose --update",
    "perf:check": "tsx scripts/performance-budget-check.ts",
    "perf:check:strict": "tsx scripts/performance-budget-check.ts --strict",
    "lighthouse:audit": "tsx scripts/lighthouse-audit.ts",
    "lighthouse:traditional": "tsx scripts/lighthouse-audit.ts --mode=traditional",
    "lighthouse:canvas": "tsx scripts/lighthouse-audit.ts --mode=canvas",
    "lighthouse:timeline": "tsx scripts/lighthouse-audit.ts --mode=timeline",
    "lighthouse:ci": "tsx scripts/lighthouse-audit.ts --ci"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "color2k": "^2.0.3",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "framer-motion": "^12.23.22",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sirv": "^3.0.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@chromatic-com/storybook": "^4.1.1",
    "@playwright/test": "^1.55.1",
    "@storybook/addon-a11y": "^9.1.9",
    "@storybook/addon-docs": "^9.1.9",
    "@storybook/addon-onboarding": "^9.1.9",
    "@storybook/addon-vitest": "^9.1.9",
    "@storybook/react-vite": "^9.1.9",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "exifr": "^7.1.3",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.4",
    "sharp": "^0.34.4",
    "storybook": "^9.1.9",
    "tailwindcss": "^4.1.13",
    "terser": "^5.44.0",
    "tsx": "^4.20.6",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4"
  }
}
